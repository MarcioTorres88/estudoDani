package youtu_tube_jogo;

import java.awt.Canvas;


public class  Game extends Canvas implements Runnable {

	@Override	
	public void run() {
		while (true) {
			update ();
			render();
			try {
				Thread, sleep(1000/60);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
			
		}
		
		
	}
}
